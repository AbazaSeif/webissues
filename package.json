{
  "name": "webissues",
  "description": "WebIssues team collaboration system",
  "version": "2.0.0",
  "author": "WebIssues Team",
  "private": true,
  "scripts": {
    "build": "npm run build:web && npm run build:electron && npm run build:main",
    "build:web": "webpack --config src/build/webpack.config.js --env.production --hide-modules",
    "build:electron": "webpack --config src/build/webpack.config.js --env.electron --env.production --hide-modules",
    "build:main": "webpack --config app/src/build/webpack.config.js --env.production --hide-modules",
    "dev:web": "webpack-dev-server --config src/build/webpack.config.js",
    "dev:electron": "webpack-dev-server --config src/build/webpack.config.js --env.electron",
    "start": "electron ./app/assets/js/main.min.js",
    "start:dev": "electron ./app/src/main.js",
    "package": "node ./app/src/build/package.js",
    "package:win32": "node ./app/src/build/package.js win32 ia32",
    "package:darwin": "node ./app/src/build/package.js darwin x64",
    "package:linux-ia32": "node ./app/src/build/package.js linux ia32",
    "package:linux-x64": "node ./app/src/build/package.js linux x64"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "path-to-regexp": "^2.1.0",
    "vue": "^2.5.9",
    "vue-i18n": "^7.3.2",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "electron": "^2.0.0",
    "electron-packager": "^12.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
