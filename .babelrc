{
  "presets": [
    [ "env", {
      "targets": {
        "browsers": [ "defaults" ]
      },
      "modules": false,
      "useBuiltIns": true
    } ]
  ],
  "plugins": [ "transform-object-rest-spread" ],
  "comments": false,
  "env": {
    "test": {
      "presets": [
        [ "env", {
          "targets": {
            "node": 8
          },
          "modules": "commonjs",
          "useBuiltIns": true
        } ]
      ],
      "plugins": [
        [ "module-resolver", {
          "alias": {
            "@": "./src"
          }
        } ]
      ]
    }
  }
}
